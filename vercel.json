{
  "version": 2,
  "name": "india-music-insights",
  "builds": [
    {
      "src": "frontend/indie-melody-pulse-89/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/indie-melody-pulse-89/dist/$1"
    }
  ],
  "buildCommand": "cd frontend/indie-melody-pulse-89 && npm install && npm run build",
  "outputDirectory": "frontend/indie-melody-pulse-89/dist",
  "installCommand": "cd frontend/indie-melody-pulse-89 && npm install"
}
